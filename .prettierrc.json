{
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "always",
    "requirePragma": false,
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "useTabs": false,
    "trailingComma": "none"
}
