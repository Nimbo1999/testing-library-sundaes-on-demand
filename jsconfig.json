{
    "compilerOptions": {
      "module": "ES2020",
      "target": "ES2020",
      "baseUrl": "src",
      "checkJs": true,
      "paths": {
          "pages": ["./pages/*"],
          "contexts": ["./contexts/*"],
          "constants": ["./constants/*"],
          "test-utils": ["./test-utils/*"],
          "utils": ["./utils/*"],
      },
      "jsx": "react-jsx",
      "moduleResolution": "node"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
